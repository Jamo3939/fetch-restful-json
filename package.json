{
  "name": "fetch-restful-json",
  "version": "0.0.0",
  "main": "./src/index.js",
  "description": "Simple RESTful APIs for fetch clients.",
  "scripts": {
    "lint": "eslint *.js src",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "engines": {
    "node": "7.1.0",
    "npm": "3.10.9"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.3.2",
    "eslint": "3.19.0",
    "eslint-config-standard": "7.1.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "2.1.1",
    "fetch-mock": "5.10.0",
    "jest": "19.0.2",
    "rollup": "0.41.6",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleDirectories": [
      "<rootDir>/src",
      "node_modules"
    ]
  }
}
